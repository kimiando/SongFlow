<div class="game-page-body">
  <div class="game-page-navbar-container">
    <div class="game-page-navbar-arrow">
      <%= link_to user_homepage_index_path(current_user) do%>
        <i class="fa-solid fa-arrow-left fa-arrow-left-game-page"></i>
      <% end %>
    </div>
    <div class="game-page-navbar-logo">
      <%= image_tag("logo.png", class: "logo-size") %>
      <span class="game-page-navbar-title">SongFlow</span>
    </div>
    <div class="game-page-navbar-item">
      <p>hi</p>
    </div>
  </div>
  <div class="game-page">
    <div class="game-page-container">
      <%= render "songs/songs_random" %>
      <%= link_to random_song_path, class: "button-5" do %>
        Try Again
      <% end %>
      <%# modal for adding a playlist %>
    </div>
  </div>
</div>
